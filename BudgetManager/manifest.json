{
  "manifest_version": 2,
  "name": "Budget Manager",
  "version": "1.0",
  "description": "A simple budget manager, credits to gopinav on github",
  "icons": {
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
  },
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_popup": "popup.html"
  },
  "permissions":[
    "storage"
  ]
}
